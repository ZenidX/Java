import java.awt.*;
import java.awt.event.*;

public class ExempleGrafic implements ActionListener{
	Button b,b2;
	public static void main(String[] args) {
		new ExempleGrafic();
	}
	ExempleGrafic() {
		Frame f=new Frame("Finestra");
		f.setLayout(new FlowLayout());
		
		b=new Button("bot贸");
		f.add(b);b.addActionListener(this);
		b2=new Button("bot贸2");
		f.add(b2);b2.addActionListener(this);
		
		f.setSize(300, 300);
		f.setVisible(true);
	}
	public void actionPerformed(ActionEvent e) {
		if (e.getSource()==b) {	//Object==Button
			System.out.println("Bot贸1 premut");
		}
		if (e.getSource()==b2) {
			System.out.println("Bot贸2 premut");
		}
		
	}

}